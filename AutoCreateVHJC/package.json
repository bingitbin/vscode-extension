{
    "name": "AautoCreateVHJC",
    "displayName": "AautoCreateVHJC",
    "description": "auto create vue html js css",
    "version": "0.0.1",
    "publisher": "xbzhang",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.addFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
        {
            "command": "extension.addFile",
            "title": "Add Html、Css、Js、Vue File"
        }
    ],
    "menus": {
			"explorer/context": [
				{
					"command": "extension.addFile",
                    "group": "extension",
                    "when":"explorerResourceIsFolder"
				}
			]
		}
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}